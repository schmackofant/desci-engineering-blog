---
export interface Props {
	videoId: string;
}

const { videoId } = Astro.props;
const youtubeUrl = `https://www.youtube-nocookie.com/embed/${videoId}?rel=0`
---

<div class="video-container">
  <iframe class="w-full aspect-video" src={youtubeUrl} frameborder="0" allowfullscreen="allowfullscreen"></iframe>
</div>